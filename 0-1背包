 #include <bits/stdc++.h>

using namespace std;

int w[105], val[105];

int np[105];
int main()
{
    int t, m, res = -1;
    cin >> t >> m;
    for (int i = 1; i <= m; i++)
        cin >> w[i] >> val[i];

    for (int i = 1; i <= m; i++)     //物品
        for (int j = t; j >= w[i]; j--) //容量
        {
                np[j]=np[j-w[i]]+val[i]>np[j]?np[j-w[i]]+val[i]:np[j];    
        }
    for (int i = 1; i <= t; i++)
    {
    
        cout << np[i]<<' ';
    }
    system("pause");
    return 0;
} 
